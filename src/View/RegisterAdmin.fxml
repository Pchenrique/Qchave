         <padding>
            <Insets left="75.0" top="3.0" />
         </padding>
      </Label>
      <TextField fx:id="nome_completo" layoutX="72.0" layoutY="131.0" prefHeight="35.0" prefWidth="290.0" promptText="Ex: Tiago Dias Moreira" />
      <TextField fx:id="cpf" layoutX="74.0" layoutY="198.0" prefHeight="35.0" prefWidth="290.0" promptText="EX: 22234576520" />
      <Button fx:id="btn_cadastrar_admin" layoutX="74.0" layoutY="315.0" mnemonicParsing="false" onAction="#registrarAdmin" prefHeight="35.0" prefWidth="290.0" style="-fx-background-color: #00c444;" text="Cadastrar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
