      </Label>
      <Label layoutX="74.0" layoutY="237.0" text="Token">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="token" layoutX="74.0" layoutY="261.0" prefHeight="35.0" prefWidth="150.0" promptText="EX: 1234 ou 123456" />
      <Button layoutX="74.0" layoutY="360.0" mnemonicParsing="false" onAction="#backPage" prefHeight="35.0" prefWidth="290.0" style="-fx-background-color: #de2525;" text="Cancelar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="153.0" layoutY="475.0" text="IFCE - CAMPUS CEDRO">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
